  "name": "My Theme",
  "tokenColors": [
    {
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.block.documentation",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#858DB7"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#eeeeee"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#E1112C"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "fontStyle": "",
        "foreground": "#e09ce9"
      }
