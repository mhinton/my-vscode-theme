    "activityBar.border": "#0F121A",
    "breadcrumb.background": "#141923",
    "breadcrumb.foreground": "#EAEBEC",
    "editor.background": "#171e29",
    "editor.foreground": "#E8E8EF",
    "editorLineNumber.activeForeground": "#FFF5CD",
    "editorLineNumber.foreground": "#6A83A7",
    "editor.lineHighlightBackground": "#171e29",
    "editorGroupHeader.tabsBackground": "#171e29",
    "editorGroupHeader.tabsBorder": "#0F121A",
    "editorGutter.background": "#1C2532",
    "list.activeSelectionBackground": "#323E4F",
    "list.inactiveFocusBackground": "#232D3D",
