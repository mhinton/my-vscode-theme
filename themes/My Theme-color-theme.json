    "input.background": "#1C2532",
    "input.foreground": "#EEEEEE",
    "input.placeholderForeground": "#8EACC5",
    "input.border": "#0F121A"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.block.documentation",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#989fa7"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E1112C"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#e09ce9",
        "fontStyle": ""
      }
    },
    {
      "name": "Key",
      "scope": "meta.object-literal.key.js",
      "settings": {
        "foreground": "#8afcf6"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#8EACC5"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation.separator.inheritance.php",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#e09ce9"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#e09ce9"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#52C1F9"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "variable.other.readwrite.alias",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#FFF5CD"
      }
    },
    {
      "name": "Number, Constant, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#ff6340"
      }
    },
    {
      "name": "Function Argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#DCA48E"
      }
    },
    {
      "name": "Support",
      "scope": "support.constant",
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "name": "Handlebars Keywords",
      "scope": [
        "meta.function.block.start.handlebars support.constant.handlebars",
        "meta.function.block.end.handlebars support.constant.handlebars"
      ],
      "settings": {
        "foreground": "#A9C7DF"
      }
    },
    {
      "name": "Handlebars Separators",
      "scope": [
        "meta.function.inline.other.handlebars support.constant.handlebars"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Handlebars Variable",
      "scope": "variable.parameter.handlebars",
      "settings": {
        "foreground": "#e9d4a6"
      }
    },
    {
      "name": "Handlebars Args",
      "scope": ["meta.function.inline.other.handlebars"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Vue Expression",
      "scope": "expression.embedded.vue variable.other.readwrite.js",
      "settings": {
        "foreground": "#e9d4a6"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
